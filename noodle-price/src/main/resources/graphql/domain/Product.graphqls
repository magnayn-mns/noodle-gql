type Product @key(fields: "id") {
    id: ID!
    test:String
    listPrice(param:ListPriceFilter): [Pricing!]!
    intermediate: Intermediate!
}

type Intermediate {
    listPrice(param:ListPriceFilter): [Pricing!]!
}

type Pricing {

    location: Location
    price: Price
}

input ListPriceFilter {
    location: String
}
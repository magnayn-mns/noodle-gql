type Query {
    """
    Get priced quote for given quote Id

    Equivalent to GET /quote/{quoteId}
    """
    quote(
        """ID of quote to return"""
        id: ID!
    ): Quote
}

type Fish @key(fields: "id") {
    id: ID!
    size: Int @external
    weight: Int @external
    shippingEstimate: String @requires(fields: "size weight")
}




